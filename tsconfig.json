{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","./components.d.ts"],
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "removeComments": true,
    "noEmit": false,
    "noEmitOnError": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
